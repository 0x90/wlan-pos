#!/bin/bash
logs=$*
if [ "$logs" == "" ]; then
    logdir=~/tmp/log
    logfiles=wpp.log*
    logs=$logdir/$logfiles
fi

hits_tot=`grep 'PosReq' $logs |wc -l`
hits_ok=`grep 'ErrCode="102"' $logs |wc -l`
rate_ok=`echo "scale=4;($hits_tot-$hits_ok)/$hits_tot*100" |bc`
echo "positioning success rate: ${rate_ok}%"
